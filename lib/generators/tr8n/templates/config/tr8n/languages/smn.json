{
  "locale": "smn",
  "english_name": "Inari Sami",
  "context_rules": {
    "date": {
      "keys": ["past", "present", "future"]
    },
    "gender_list": {
      "keys": ["one", "other"],
      "rules": {
        "one": {"size": "(= 1 @size)"},
        "other": {"size": "(!= 1 @size)"}
      }
    },
    "gender": {
      "keys": ["male", "female", "unknown"]
    },
    "number": {
      "keys": ["one", "two", "other"],
      "rules": {
        "one":    {"rule": "(= 1 @n)", "description": "{n} is 1", "examples": "1"},
        "two":    {"rule": "(= 2 @n)", "description": "{n} is 2", "examples": "2"},
        "other":  {"examples": "0, 3-999; 1.2, 2.07, 3.94..."}
      }
    }
  },
  "language_cases": {
  }
}
