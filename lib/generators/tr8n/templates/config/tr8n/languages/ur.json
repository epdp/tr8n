{
  "locale": "ur",
  "english_name": "Urdu",
  "native_name": "اردو",
  "right_to_left": true,
  "google_key": "ur",
  "context_rules": {
    "date": {
      "keys": ["past", "present", "future"]
    },
    "gender_list": {
      "keys": ["one", "other"],
      "rules": {
        "one": {"rule": "(= 1 @size)", "description": "contains 1 person"},
        "other": {"rule": "(!= 1 @size)", "description": "contains at least 2 people"}
      }
    },
    "gender": {
      "keys": ["male", "female", "unknown"]
    },
    "number": {
      "keys": ["one", "other"],
      "rules": {
        "one":    {"rule": "(= 1 @n)", "description": "{n} is 1", "examples": "1"},
        "other":  {"examples": "0, 2-999; 1.2, 2.07..."}
      }
    }
  },
  "language_cases": {
  }
}
